  },
  "homepage": "https://github.com/shomrkm/shochan_ai#readme",
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "@types/js-yaml": "^4.0.9",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "openai": "^6.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
