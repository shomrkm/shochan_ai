{
  "name": "shochan_ai",
  "version": "0.0.1",
  "description": "AI Agent for me.",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "dev": "pnpm --filter @shochan_ai/cli dev",
    "cli": "pnpm --filter @shochan_ai/cli start",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "biome format --write .",
