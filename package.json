    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shomrkm/shochan_ai.git"
  },
  "keywords": [],
